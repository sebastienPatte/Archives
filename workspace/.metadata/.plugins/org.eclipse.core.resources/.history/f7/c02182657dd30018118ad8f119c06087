package vector;

public class vector {
	double x,y,z;
	vector(double x, double y, double z){
		this.x=x;
		this.y=y;
		this.z=z;
	}
	
	@Override
    public String toString(){
        return "(" + this.x + ";" + this.y + ";" + this.z + ")";
    }

	public static void main(String[] args){
		vector v1 = new vector(1,1,1);
		System.out.println(v1);
	}
}